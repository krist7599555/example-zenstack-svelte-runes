{
  "name": "example-zenstack-svelte-runes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate": "zenstack generate --output .zenstack; rm -rf .zenstack/**.{js,d.ts}",
    "postinstall": "patch-package; bun run generate",
    "clean": "trash node_modules .svelte-kit .zenstack",
    "db:push": "prisma db push --schema ./.zenstack/schema.prisma"
  },
  "devDependencies": {
    "@fontsource/noto-sans-thai": "^5.0.13",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.106",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/vite": "^4.0.0-alpha.13",
    "@tanstack/svelte-query": "^5.28.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.56.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@zenstackhq/tanstack-query": "^2.0.0-beta.5",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "jsonwebtoken": "^9.0.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.2",
    "prisma": "^5.12.1",
    "svelte": "^5.0.0-next.94",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^4.0.0-alpha.13",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "zenstack": "2.0.0-beta.5"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "@zenstackhq/runtime": "2.0.0-beta.5",
    "@zenstackhq/server": "^1.12.0"
  }
}
